<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="GardenOfFearLevelEditor.NewDocumentPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <StackPanel Orientation="Horizontal">
            <NumberBox x:Name="WidthBox" Header="Width" Value="8" AcceptsExpression="False" Margin="8,0"/>
            <NumberBox x:Name="HeightBox" Header="Height" Value="8" AcceptsExpression="False" Margin="8,0"/>
        </StackPanel>
    </StackPanel>

</Page>
